{
   "version": "v0.0.1",
   "license": "MIT",
   "keywords": [
      "zesty-io",
      "headless-cms",
      "typescript",
      "zesty",
      "cms"
   ],
   "main": "dist/index.js",
   "typings": "dist/index.d.ts",
   "files": [
      "dist",
      "src"
   ],
   "engines": {
      "node": ">=10"
   },
   "scripts": {
      "start": "tsdx watch",
      "build": "tsdx build",
      "test": "tsdx test --passWithNoTests",
      "lint": "tsdx lint",
      "prepare": "tsdx build",
      "size": "size-limit",
      "analyze": "size-limit --why",
      "release": "release-it"
   },
   "peerDependencies": {
      "react": ">=16"
   },
   "husky": {
      "hooks": {
         "pre-commit": "tsdx lint"
      }
   },
   "prettier": {
      "endOfLine": "lf",
      "printWidth": 90,
      "tabWidth": 3,
      "trailingComma": "all",
      "semi": false,
      "singleQuote": false,
      "bracketSpacing": true,
      "arrowParens": "always",
      "bracketSameLine": false
   },
   "name": "@darwin808/explorer-release123",
   "author": "darwin.apolinario12",
   "module": "dist/zesty-explorer.esm.js",
   "size-limit": [
      {
         "path": "dist/zesty-explorer.cjs.production.min.js",
         "limit": "10 KB"
      },
      {
         "path": "dist/zesty-explorer.esm.js",
         "limit": "10 KB"
      }
   ],
   "devDependencies": {
      "@size-limit/preset-small-lib": "^7.0.8",
      "@types/react": "^18.0.0",
      "@types/react-dom": "^18.0.0",
      "babel-plugin-module-resolver": "^4.0.0",
      "husky": "^7.0.4",
      "react": "^18.0.0",
      "react-dom": "^18.0.0",
      "release-it": "^14.14.2",
      "size-limit": "^7.0.8",
      "tsdx": "^0.14.1",
      "tslib": "^2.3.1",
      "typescript": "^4.6.3"
   },
   "dependencies": {
      "@material-ui/core": "^4.12.4",
      "fuse.js": "^6.5.3",
      "react-code-blocks": "^0.0.9-0",
      "react-json-view-ssr": "^1.19.1"
   },
   "moduleDirectories": [
      "node_modules",
      "src"
   ]
}
